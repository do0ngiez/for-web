<%- include('./partials/_top_admin') %>

<h2>User Contact Tracing Form Details</h2>

 <!-- USER DETAILS -->
 <div class="container">
    <div class="bg-addu pt-2 pl-2 pr-2 pb-1">
      <h4>User Details</h4>
    </div>
    <div class="container border pt-3 pl-4 pr-4 pb-4">
      <div>
        <div class="row">
          <div class="col-md-6">
            <div class="form-group">
              <label for="firstName"> First Name </label>
              <input type="text" class="form-control" id="firstName" name="firstName"
                value="<%= user.firstName %>" readonly />
            </div>
          </div>
          <div class="col-md-6">
            <div class="form-group">
              <label for="lastName"> Last Name </label>
              <input type="text" class="form-control" id="lastName" name="lastName" value="<%= user.lastName %>"
                readonly />
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col-md-6">
            <div class="form-group">
              <label for="address"> Address </label>
              <input type="text" class="form-control" id="address" name="address" value="<%= user.address %>"
                readonly />
            </div>
          </div>
          <div class="col-md-6">
            <div class="form-group">
              <label for="phoneNumber"> Phone Number </label>
              <input type="number" class="form-control" id="phoneNumber" name="phoneNumber"
                value="<%= user.phoneNumber %>" readonly />
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- WHO LIVES WITH YOU PORTION -->
    <div class="container">
      <div class="bg-addu pt-2 pl-2 pr-2 pb-1">
        <h4>Who lives with you?</h4>
      </div>
      <div id="livesWithContainer" class="container border pt-3 pl-4 pr-4 pb-4">
        <div id="livesWithForm">
          <div>
              <% ctLivesWiths.forEach((livesWith, index) => { %>
            <div class="row">
              <div class="col-md-4">
                <label for="livesWith[<%= index %>][firstName]">First Name</label>
                <input type="text" class="form-control" id="livesWith[<%= index %>][firstName]" name="livesWith[<%= index %>][firstName]"
                  value="<%= livesWith.firstName %>" readonly>
              </div>
              <div class="col-md-4">
                <label for="livesWith[<%= index %>][lastName]">Last Name</label>
                <input type="text" class="form-control" id="livesWith[<%= index %>][lastName]" name="livesWith[<%= index %>][lastName]"
                  value="<%= livesWith.lastName %>" readonly>
              </div>
              <div class="col-md-3">
                <label for="livesWith[<%= index %>][phoneNumber]">Phone Number</label>
                <input type="text" class="form-control phoneNumber" id="livesWith[<%= index %>][phoneNumber]" name="livesWith[<%= index %>][phoneNumber]"
                  value="<%= livesWith.phoneNumber %>" title="Please enter number only" minlength="11" maxlength="11" pattern="[0-9]+" readonly>
              </div>

            </div>
            <% }) %>
          </div>
        </div>

      </div>
    </div>

    <div class="container">
      <div class="bg-addu pt-2 pl-2 pr-2 pb-1">
        <h4>Who have you been around (within 6 feet for more than 15 minutes) in the last 10 days?</h4>
      </div>
      <div id="beenAroundContainer" class="container border pt-3 pl-4 pr-4 pb-4">
        <div id="beenAroundForm">
          <div>
            <% ctBeenArounds.forEach((beenAround, index) => { %>

            <div class="row">
              <div class="col-md-6">
                <label for="beenAround[<%= index %>][firstName]">First Name</label>
                <input type="text" class="form-control" id="beenAround[<%= index %>][firstName]" name="beenAround[<%= index %>][firstName]"
                  value="<%= beenAround.firstName %>" readonly>
              </div>
              <div class="col-md-6">
                <label for="beenAround[<%= index %>][lastName]">Last Name</label>
                <input type="text" class="form-control" id="beenAround[<%= index %>][lastName]" name="beenAround[<%= index %>][lastName]"
                  value="<%= beenAround.lastName %>" readonly>
              </div>
            </div>

            <div class="row">
              <div class="col-md-6">
                <label for="beenAround[<%= index %>][dateLastSaw]">Date you last saw them</label>
                <input type="text" class="form-control datepicker" id="beenAround[<%= index %>][dateLastSaw]"
                  name="beenAround[<%= index %>][dateLastSaw]" value="<%= beenAround.dateLastSaw %>" readonly>
              </div>
              <div class="col-md-5">
                <label for="beenAround[<%= index %>][whereLastSaw]">Where you last saw them</label>
                <input type="text" class="form-control" id="beenAround[<%= index %>][whereLastSaw]"
                  name="beenAround[<%= index %>][whereLastSaw]" value="<%= beenAround.whereLastSaw %>" readonly>
              </div>

            </div>
            <% }) %>
          </div>
        </div>

      </div>
    </div>

    <div class="container">
      <div class="bg-addu pt-2 pl-2 pr-2 pb-1">
        <h4>What have you done in the last 10 days with other people? </h4>
      </div>
      <div id="activityContainer" class="container border pt-3 pl-4 pr-4 pb-4">
        <div id="activityForm">
          <div>

            <% ctActivities.forEach((activity, index) => { %>

            <div class="row">
              <div class="col-md-9">
                <label for="activity[<%= index %>][activity]">Activity</label>
                <input type="text" class="form-control" id="activity[<%= index %>][activity]" name="activity[<%= index %>][activity]" value="<%= activity.activity %>"
                  readonly>
              </div>
              <div class="col-md-3">
                <label for="activity[<%= index %>][date]">Date</label>
                <input type="text" class="form-control datepicker" id="activity[<%= index %>][date]" name="activity[<%= index %>][date]"
                  value="<%= activity.date %>" readonly>
              </div>
            </div>

            <div class="row">
              <div class="col-md-11">
                <label for="activity[<%= index %>][location]">Location</label>
                <input type="text" class="form-control" id="activity[<%= index %>][location]" name="activity[<%= index %>][location]" value="<%= activity.location %>"
                readonly>
              </div>

            </div>

            <% }) %>
          </div>
        </div>

      </div>
    </div>
    <!-- BOTTOM PORTION -->
    <div class="container">
      <div class="container">
        <div class="row border-bottom border-left border-right p-2" style="background-color: #e6e6e6;">
          <div class="col-sm-6">
            <h3 class="color-adduOrange"><b>Things to think about:</b></h3>
            <p>
            <ul>
              <li>Gone to work or school?</li>
              <li>Met with others (e.g. eaten out at a restaurant, gone out for drinks, exercised to a gym, had friends
                in your house, volunteered, strolling in your village/subdivision, park)</li>
              <ul>
                </p>
          </div>
          <div class="col-sm-6">
            <div class="mt-5">
              <p>
              <ul>
                <li>Gone to a store in person (eg. grocery store, mall)</li>
                <li>Gone in-person appointments (e.g. salon, barber, doctor’s/dentist’s office)</li>
                <li>Ridden in a car with others (e.g. Taxi) or took public transportation?</li>
                <ul>
                  </p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

<%- include('./partials/_bottom') %>